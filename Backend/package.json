{
  "name": "template-services",
  "version": "1.0.0",
  "description": "template services",
  "license": "ISC",
  "author": "parth dangaroshiya",
  "type": "commonjs",
  "main": "src/server.ts",
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md,mjs}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "test:watch": "jest --passWithNoTests --watch --coverage --all --runInBand",
    "test": "jest --no-cache --coverage --all --runInBand",
    "build": "tsc",
    "start": "ts-node dist/server.js",
    "dev": "nodemon --legacy-watch src/server.ts",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "typeorm": "npm run build && npx typeorm-ts-node-esm -d dist/database/datasourcemigration.js",
    "migration:show": "npm run typeorm migration:show",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate",
    "migration:run": "typeorm-ts-node-commonjs migration:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "arg": "^4.1.3",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "create-require": "^1.1.1",
    "diff": "^4.0.2",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "make-error": "^1.3.6",
    "mongoose": "^8.18.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rsa-pem-to-jwk": "^1.1.3",
    "typeorm": "^0.3.26",
    "undici-types": "^7.10.0",
    "uuid": "^11.1.0",
    "v8-compile-cache-lib": "^3.0.1",
    "winston": "^3.17.0",
    "yn": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "express-validator": "^7.2.1",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "jest": "^30.0.5",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}
